<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

          <hibernate-mapping>
          <class name="com.hello.mypackage.StockOut" table="stock_out">
	          <id name="id">
	          <generator class="assigned"></generator>
	          </id>
			<property name="organization_code"></property>
			<property name="organization_name"></property>
			<property name="entity"></property>
			<property name="transaction_id"></property>
			<property name="item_number"></property>
			<property name="item_description"></property>
			<property name="transaction_date"></property>
			<property name="ir_line_attribute1"></property>
			<property name="quantity"></property>
			<property name="uom"></property>
			<property name="lot_number"></property>
			<property name="claim_price"></property>
			<property name="cost"></property>
			<property name="wht_rate"></property>
			<property name="output_vat_rate"></property>
			<property name="ir_number"></property>
			<property name="ir_line"></property>
			<property name="sub_inventory"></property>
			<property name="sub_inventory_description"></property>
			<property name="transaction_type"></property>
			<property name="mkt_activity_name"></property>
          </class>
          
          </hibernate-mapping>